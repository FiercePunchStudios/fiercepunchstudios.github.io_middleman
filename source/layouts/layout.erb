<!doctype html>
<html class="no-js" lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <!-- Use title if it's in the page YAML frontmatter -->
  <title><%= ["Fierce Punch Studios", data.page.title].compact.join(' | ') %></title>

  <%= stylesheet_link_tag "app", "style" %>
  <%= javascript_include_tag "modernizr" %>
  <%= favicon_tag 'images/favicon.png' %>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-76306805-1', 'auto');
    ga('send', 'pageview');

  </script>
</head>

<body class="<%= page_classes %>">
  <div id="container-row" class="off-canvas-wrap" data-offcanvas>
    <div id="container-column" class="inner-wrap">
      <%= partial 'partials/header' %>
      <main id="main-card">
        <%= yield %>
      </main>
      <%= partial 'partials/footer' %>

      <a class="exit-off-canvas"></a>
    </div>
  </div>
  <%= javascript_include_tag  "all" %>
</body>
</html>
